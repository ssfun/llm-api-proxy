{
  "version": 2,
  "functions": {
    "api/gateway.ts": {
      "runtime": "edge"
    },
    "api/background.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "^/gateway/.*$",
      "dest": "/api/gateway.ts"
    },
    {
      "src": "^/background$",
      "dest": "/api/background.ts"
    }
  ]
}
